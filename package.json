{
  "name": "pr-yarn-berry",
  "packageManager": "yarn@3.6.1",
  "sideEffects": false,
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check --watch",
    "start": "cross-env NODE_ENV=production webpack serve --config config/webpack.prod.js",
    "build": "yarn type-check && cross-env NODE_ENV=production webpack --progress --config config/webpack.prod.js",
    "dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "storybook": "storybook dev -p 6006",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@emotion/babel-plugin": "^11.11.0",
    "@emotion/css": "^11.11.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@storybook/addon-essentials": "^7.0.24",
    "@storybook/addon-interactions": "^7.0.24",
    "@storybook/addon-links": "^7.0.24",
    "@storybook/blocks": "^7.0.24",
    "@storybook/preset-create-react-app": "^7.0.24",
    "@storybook/react": "^7.0.24",
    "@storybook/react-webpack5": "^7.0.24",
    "@storybook/testing-library": "^0.2.0",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/node": "^20.3.3",
    "@types/pnpapi": "^0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "@yarnpkg/sdks": "^3.0.0-rc.47",
    "babel-loader": "^9.1.2",
    "babel-plugin-styled-components": "^2.1.4",
    "babel-preset-react-app": "^10.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "esbuild-loader": "^3.0.1",
    "eslint": "^8.43.0",
    "eslint-plugin-react": "^7.32.2",
    "fork-ts-checker-webpack-plugin": "^8.0.0",
    "html-webpack-plugin": "^5.5.3",
    "husky": ">=7",
    "lint-staged": ">=10",
    "msw": "^1.2.2",
    "pinst": ">=2",
    "pnpapi": "^0.0.0",
    "prettier": "^2.8.8",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.14.1",
    "storybook": "^7.0.24",
    "styled-components": "^6.0.1",
    "terser-webpack-plugin": "^5.3.9",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "webpack": "^5.88.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@tanstack/react-query": "^4.29.19",
    "axios": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "msw": {
    "workerDirectory": "public"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{ts,tsx,css}": "prettier --write"
  }
}
